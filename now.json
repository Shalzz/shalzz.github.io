{
    "version": 2,
    "name": "jain.sh",
    "alias": [
        "jain.sh"
    ],
    "routes": [
        { "src": "/.well-known/openpgpkey/hu(/)?", "dest": "/404.html", "status": 404 },
        { "src": "/.well-known/(.*)", "dest": "/well-known/$1" },
        { "src": "/static/(.*)", "dest": "/static/$1" },
        { "src": "/(.*)", "status": 301, "headers": { "Location": "https://shaleenjain.com/$1" }}
    ]
}
